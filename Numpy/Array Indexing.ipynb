    "import numpy as np\n",
    "a=[]\n",
    "n=int(input(\"Enter Size of Array: \"))\n",
    "for i in range(n):\n",
    "    val=int(input(\"Enter Values of Array\"))\n",
    "    a.append(val)\n",
    "arr=np.array(a)\n",
    "sum=0\n",
    "for i in range(arr.size):\n",
    "    print(arr[i])\n",
    "    sum=sum+arr[i]\n",
    "print(\"Sum of array elements is \", sum)    "
